# coding: utf8

def generate_phrase(class_number):
    if class_number == 1:
        return u'лежит'
    elif class_number == 2:
        return u'лежит'
    elif class_number == 3:
        return u'лежит'
    elif class_number == 4:
        return u'лежит'
    elif class_number == 5:
        return u'лежит'
    elif class_number == 6:
        return u'лежит
    elif class_number == 7:
        return u'лежит'
    elif class_number == 8:
        return u'играет'
    elif class_number == 9:
        return u'лежит'
    else:
        return u'не определен'

